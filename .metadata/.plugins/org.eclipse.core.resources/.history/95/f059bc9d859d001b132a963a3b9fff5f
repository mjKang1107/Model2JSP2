class Parent{
	public void pprn(){
		System.out.println("부모-pprn()");
		
	}
	
	
	
}

class Child extends Parent{
	public void cprn(){
		System.out.println("자식-cprn()");
		//부모의 기능 사용 가능 pprn(),cprn()-둘다 사용 가능
	}
	
	
}

public class Test2 {

	public static void main(String[] args) {
		Parent p = new Parent();
		p.pprn();
		//p.cprn(); 부모클래스에는 자식객체거를 쓸수 없어서 에러남
		Child c = new Child();
		c.cprn();
		c.pprn();
	}

}
