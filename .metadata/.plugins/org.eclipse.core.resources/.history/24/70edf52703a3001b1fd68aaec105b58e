import java.util.ArrayList;
import java.util.*;

public class ArrayListTest {
	public static void main(String[] args) {
		// 컬렉션: 자료구조, 제네릭
		
		// int[] arr : 고정길이 배열
		// ArrayList : 가변길이 배열(동적배열)
		// => 데이터의 크기 자동으로 변경 , list 계열 (순서를 기억(index),중복허용) 

		ArrayList arr = new ArrayList();
				//제네릭
		
		//데이터 입력
		//arr[0] = 100; (X)
		arr.add(100); // (O)
		
		
		
		
		
	}

}
