import java.util.ArrayList;
import java.util.*;

public class ArrayListTest {
	public static void main(String[] args) {
		// 컬렉션: 자료구조, 제네릭
		
		// int[] arr : 고정길이 배열
		// ArrayList : 가변길이 배열(동적배열)
		// => 데이터의 크기 자동으로 변경 , list 계열 (순서를 기억(index),중복허용) 

		ArrayList arr = new ArrayList();
				//제네릭
		
		//데이터 입력
		//arr[0] = 100; (X)
	
		arr.add(100); // wrapper클래스(8개) : 기본형데이터타입을 참조형 변경해줌 -> integer타입
		arr.add("hellow"); //Object에 String을 담을수 있어서 가능(업캐스팅 일어남) -> 다른타입도 다 가능
		arr.add(3.1313);
		arr.add(true);
		
		//상속은 참조형만 가능하다.
		
		System.out.println(arr); // [100, hellow, 3.1313, true] 넣은 순서대로 잘 들어가있음(index) 
		
		
		
		
	}

}
